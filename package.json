{
    "name": "nu-disco",
    "displayName": "Nu Disco Theme",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "scripts": {
        "build": "node build.js",
        "watch": "npm run build && chokidar 'tokens/**/*.json5' 'theme/**/*.json5' -c 'npm run build'"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Nu Disco Dark",
                "uiTheme": "vs-dark",
                "path": "./dist/nu-disco-dark.color-theme.json"
            },{
                "label": "Nu Disco Light",
                "uiTheme": "vs",
                "path": "./dist/nu-disco-light.color-theme.json"
            }
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "chokidar-cli": "^2.0.0",
        "style-dictionary": "^2.8.2"
    }
}
