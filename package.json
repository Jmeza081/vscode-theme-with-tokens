{
    "name": "nu-disco",
    "displayName": "Nu Disco Theme",
    "description": "A VS Code theme that boogies.",
    "publisher": "dbanksdesign",
    "license": "MIT",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/dbanksdesign/nu-disco-vscode-theme"
    },
    "engines": {
        "vscode": "^1.38.0"
    },
    "scripts": {
        "build": "node build.js",
        "watch": "npm run build && chokidar 'tokens/**/*.json5' 'theme/**/*.json5' -c 'npm run build'",
        "publish": "vsce publish"
    },
    "galleryBanner": {
        "color": "#0C001F",
        "theme": "dark"
    },
    "icon": "images/icon.png",
    "categories": [
        "Themes"
    ],
    "keywords": [
        "Nu Disco",
        "Dark Theme",
        "Light Theme",
        "Style Dictionary"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Nu Disco Dark",
                "uiTheme": "vs-dark",
                "path": "./dist/nu-disco-dark.color-theme.json"
            },{
                "label": "Nu Disco Light",
                "uiTheme": "vs",
                "path": "./dist/nu-disco-light.color-theme.json"
            }
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "chokidar-cli": "^2.0.0",
        "style-dictionary": "^2.8.2"
    }
}
